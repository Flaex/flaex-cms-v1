{
  "connection": "default",
  "collectionName": "profile",
  "info": {
    "name": "profile",
    "description": "User profile information"
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "name": {
      "unique": true,
      "required": true,
      "type": "string"
    },
    "avatar": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "summary": {
      "required": true,
      "type": "text"
    },
    "story": {
      "required": true,
      "type": "text"
    },
    "user": {
      "model": "user",
      "via": "profile",
      "plugin": "users-permissions"
    },
    "headline": {
      "required": true,
      "type": "string"
    }
  }
}